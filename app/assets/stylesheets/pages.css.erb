/* POR UP */

.s0_popUp{
	width: 100%; height: 100%;
	position: fixed;
	background: rgba(0,0,0,.75);
	z-index: 600;
	display: none;
	top:0px;
}
.s0_popUp-boxTxt{
	width: 85%;
	max-width: 800px;
	max-height: 85%;
	background: #FFF;
	position: absolute;
	top:50%; left: 50%;
	transform:  translate(-50%, -50%);
	    -webkit-transform:  translate(-50%, -50%);
	    -ms-transform:  translate(-50%, -50%);
	padding: 40px;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    overflow: auto;
}
.s0_popUp-boxTxt-btnCer, .s0_popUp-boxPic-btnCer{
	background: url(<%= asset_path 's0_popUp-boxTxt-btnCerrar.png' %>) no-repeat;
	width: 15px; height: 15px;
	position: absolute;
	top: 20px; right: 20px;
	cursor: pointer;
}

